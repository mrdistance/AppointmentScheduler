<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.appointmentscheduler.AppointmentEditController">
   <children>
      <Label fx:id="appointmentIDLabel" layoutX="28.0" layoutY="35.0" text="Appointment ID" />
      <Label fx:id="titleLabel" layoutX="28.0" layoutY="102.0" text="Title" />
      <Label fx:id="descriptionLabel" layoutX="28.0" layoutY="165.0" text="Description" />
      <Label fx:id="locationLabel" layoutX="28.0" layoutY="224.0" text="Location" />
      <Label fx:id="typeLabel" layoutX="28.0" layoutY="283.0" text="Type" />
      <Label fx:id="dateLabel" layoutX="311.0" layoutY="182.0" text="Date">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="startLabel" alignment="CENTER" layoutX="308.0" layoutY="266.0" text="Start Time">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="endLabel" alignment="CENTER" layoutX="311.0" layoutY="351.0" text="End Time">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="customerIDLabel" layoutX="32.0" layoutY="405.0" text="Customer ID" />
      <Label fx:id="userIDLabel" layoutX="32.0" layoutY="470.0" text="User ID" />
      <Label fx:id="contactLabel" layoutX="30.0" layoutY="342.0" text="Contact" />
      <TextField fx:id="appointmentIDField" disable="true" editable="false" layoutX="28.0" layoutY="53.0" promptText="AUTO" />
      <TextField fx:id="titleField" layoutX="28.0" layoutY="119.0" />
      <TextField fx:id="descriptionField" layoutX="28.0" layoutY="182.0" />
      <TextField fx:id="locationField" layoutX="28.0" layoutY="241.0" />
      <ChoiceBox fx:id="customerIdChoiceBox" layoutX="29.0" layoutY="428.0" />
      <TextField fx:id="userId" disable="true" editable="false" layoutX="30.0" layoutY="487.0" />
      <DatePicker fx:id="datePicker" layoutX="314.0" layoutY="207.0" onAction="#onDatePickerSelection" />
      <ChoiceBox fx:id="contactChoiceBox" layoutX="29.0" layoutY="363.0" prefWidth="150.0" />
      <ChoiceBox fx:id="startChoiceBox" layoutX="311.0" layoutY="292.0" onAction="#onStartTimeSelection" prefWidth="150.0" />
      <ChoiceBox fx:id="endChoiceBox" disable="true" layoutX="312.0" layoutY="376.0" prefWidth="150.0" />
      <Button fx:id="cancelButton" layoutX="458.0" layoutY="728.0" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="58.0" prefWidth="128.0" text="CANCEL">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="saveButton" alignment="CENTER" layoutX="323.0" layoutY="728.0" mnemonicParsing="false" onAction="#onSaveButtonClick" prefHeight="58.0" prefWidth="128.0" text="SAVE">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label fx:id="errorMessageLabel" alignment="CENTER" layoutX="28.0" layoutY="520.0" prefHeight="267.0" prefWidth="281.0" textFill="RED">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="typeChoiceBox" layoutX="28.0" layoutY="300.0" prefWidth="150.0" />
      <Label fx:id="userLabel" alignment="CENTER_RIGHT" layoutX="322.0" layoutY="14.0" prefHeight="17.0" prefWidth="264.0" text="Label" textAlignment="CENTER" />
   </children>
</AnchorPane>
