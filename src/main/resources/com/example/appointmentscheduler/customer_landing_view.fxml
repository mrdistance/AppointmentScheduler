<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="605.0" prefWidth="821.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.appointmentscheduler.CustomerLandingController">
   <children>
      <Button fx:id="homeButton" alignment="CENTER" layoutX="670.0" layoutY="528.0" mnemonicParsing="false" onAction="#onHomeButtonClick" prefHeight="63.0" prefWidth="137.0" text="HOME">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="addButton" alignment="CENTER" layoutX="175.0" layoutY="465.0" mnemonicParsing="false" onAction="#onAddButtonClick" prefHeight="63.0" prefWidth="137.0" text="ADD" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="updateButton" alignment="CENTER" layoutX="333.0" layoutY="465.0" mnemonicParsing="false" onAction="#onUpdateButtonClick" prefHeight="63.0" prefWidth="137.0" text="UPDATE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="deleteButton" layoutX="491.0" layoutY="465.0" mnemonicParsing="false" onAction="#onDeleteButtonClick" prefHeight="63.0" prefWidth="137.0" text="DELETE">
