<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.appointmentscheduler.DashboardController">
   <children>
      <Label fx:id="upcomingdisplay" alignment="CENTER" layoutX="195.0" layoutY="97.0" prefHeight="206.0" prefWidth="391.0" text="No Upcoming Appointments" wrapText="true">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Button fx:id="customersButton" layoutX="29.0" layoutY="52.0" mnemonicParsing="false" onAction="#onCustomerButtonClick" text="Customers">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="appointmentButton" layoutX="29.0" layoutY="178.0" mnemonicParsing="false" onAction="#onAppointmentButtonClick" text="Appointments">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="reportButton" layoutX="29.0" layoutY="296.0" mnemonicParsing="false" onAction="#onReportButtonClick" text="Reports">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="logoutButton" layoutX="491.0" layoutY="340.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" prefHeight="46.0" prefWidth="95.0" text="Logout" wrapText="true">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label layoutX="363.0" layoutY="113.0" />
   </children>
</AnchorPane>
