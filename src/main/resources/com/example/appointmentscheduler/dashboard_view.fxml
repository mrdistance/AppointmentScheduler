<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.appointmentscheduler.DashboardController">
   <children>
      <Label fx:id="usernamelabel" layoutX="14.0" layoutY="14.0" prefHeight="46.0" prefWidth="150.0" text="USER : " />
      <Label fx:id="upcomingdisplay" alignment="CENTER" layoutX="235.0" layoutY="175.0" prefHeight="95.0" prefWidth="290.0" text="No Upcoming Appointments" wrapText="true" />
      <Button fx:id="customersButton" layoutX="29.0" layoutY="121.0" mnemonicParsing="false" onAction="#onCustomerButtonClick" text="Customers">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="appointmentButton" layoutX="29.0" layoutY="200.0" mnemonicParsing="false" onAction="#onAppointmentButtonClick" text="Appointments">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="reportButton" layoutX="29.0" layoutY="284.0" mnemonicParsing="false" onAction="#onReportButtonClick" text="Reports">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="logoutButton" layoutX="491.0" layoutY="340.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" prefHeight="46.0" prefWidth="95.0" text="Logout" wrapText="true">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
