<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.appointmentscheduler.ReportController">
   <children>
      <Button fx:id="appointmentsButton" layoutX="14.0" layoutY="56.0" mnemonicParsing="false" onAction="#onAppointmentsButtonClick" text="Appointments" />
      <Button fx:id="contactsButton" layoutX="343.0" layoutY="56.0" mnemonicParsing="false" onAction="#onContactButtonClick" text="Contact Schedules" />
      <Button fx:id="customersButton" layoutX="717.0" layoutY="56.0" mnemonicParsing="false" onAction="#onCustomersButtonClick" text="Customers" />
      <Label alignment="CENTER" layoutX="360.0" layoutY="14.0" prefHeight="17.0" prefWidth="80.0" text="REPORTS">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextArea fx:id="displayArea" editable="false" layoutX="8.0" layoutY="89.0" prefHeight="464.0" prefWidth="782.0" promptText="Select a Report to Display it's Contents">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font></TextArea>
      <Button fx:id="backButton" layoutX="745.0" layoutY="561.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
      <Label fx:id="userLabel" layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="263.0" text="Label" textAlignment="CENTER" />
   </children>
</AnchorPane>
